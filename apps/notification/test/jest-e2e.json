{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\.(t|j)s$": "ts-jest"
  },
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@app/(.*)$": "<rootDir>/../../libs/$1",
    "^libs/(.*)$": "<rootDir>/../../libs/$1"
  }
}
